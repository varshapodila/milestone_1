//Data types
let length = 16;          
let lastName = "Johnson"; 
const x = {
  firstName: "John",
  lastName: "Doe"
};           
console.log(length);
console.log(lastName);
console.log(x);

//Expression and operator
function circle(radius)
{
    this.radius = radius;
    this.area = function () 
    {
        return Math.PI * this.radius * this.radius;
    };
}
var c = new circle(3);
console.log('Area =', c.area().toFixed(2));
